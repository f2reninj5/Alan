
<a href="#main-content" id="skip-navigation">Skip Navigation</a>

<header>

	<a href="/"><img id="header-logo" src="/images/logo.svg"></img></a>

	<span id="navigation">

		<%- include(`${partialsDir}/navigation`) %>
	</span>

	<span id="account-container">

		<a id="account-dropdown" href="/dashboard">
			
			<p id="username"><%= locals.user.username %></p><img id="avatar" src="<%= `https://cdn.discordapp.com/avatars/${locals.user.id}/${locals.user.avatar}` %>">
		</a>

		<div id="account-dropdown-content">

			<div id="account-dropdown-content-list">

				<a href="/dashboard">Dashboard</a>
				<a href="/logout">Log out</a>
			</div>
		</div>
	</span>
</header>